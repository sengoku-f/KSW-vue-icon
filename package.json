{
  "name": "ksw-vue-icon",
  "version": "3.0.56",
  "license": "ISC",
  "description": "Vue3 Icons for KSW",
  "type": "module",
  "homepage": "https://sengoku-f.github.io/KSW-vue-icon",
  "repository": "https://github.com/sengoku-f/KSW-vue-icon",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:icons": "rm -rf src/icons && node bin/build.js",
    "build:icons:win": "node bin/build.js",
    "build:icons:config": "node bin/buildConfig.js",
    "build:icons:all": "npm run build:icons:config && npm run build:icons",
    "build:icons:all:win": "npm run build:icons:config && npm run build:icons:win",
    "build:code": "BUILD=packages vite build",
    "build:r-code": "rollup -c rollup.config.js",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "publish:packages": "cd packages && npm publish",
    "lint": "vite lint",
    "upv": "node ./gitHooks/updateVersion.js",
    "watch:icons": "chokidar 'src/svg/**/*' -c 'yarn build:icons:all'"
  },
  "dependencies": {
    "@vueuse/core": "^13.1.0",
    "gsap": "^3.12.5",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "autoprefixer": "^10.4.19",
    "camelcase": "^8.0.0",
    "cheerio": "^1.0.0-rc.12",
    "chokidar-cli": "^3.0.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "glob": "^10.4.1",
    "npm-run-all": "^4.1.5",
    "overlayscrollbars": "^2.10.0",
    "overlayscrollbars-vue": "^0.5.9",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rollup": "^4.21.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-empty-dir": "^1.0.5",
    "rollup-plugin-esbuild": "^6.1.1",
    "sharp": "^0.33.5",
    "svgo": "^3.2.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.2.1",
    "vite-plugin-static-copy": "^2.3.0",
    "vue": "^3.4.21",
    "vue-m-message": "^4.0.2"
  },
  "config": {
    "style": "fill",
    "size": "24"
  }
}
