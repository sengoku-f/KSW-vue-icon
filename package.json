{
  "name": "ksw-vue-icon",
  "version": "2.2.25",
  "license": "ISC",
  "description": "KSW ICON",
  "type": "module",
  "main": "packages/cjs/index.js",
  "module": "packages/es/index.js",
  "types": "packages/es/index.d.ts",
  "files": [
    "styles",
    "icons.json",
    "packages/*/*.js",
    "packages/*/*/*.js",
    "packages/*/*.d.ts",
    "packages/*/*/*.d.ts"
  ],
  "repository": "https://github.com/sengoku-f/KSW-vue-icon",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:icons": "rm -rf src/icons && node bin/build.js",
    "build:packages": "BUILD=packages vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "vite lint",
    "clean-publish": "clean-publish",
    "upv": "node ./gitHooks/updateVersion.js"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "camelcase": "^8.0.0",
    "cheerio": "^1.0.0-rc.12",
    "clean-publish": "^4.4.0",
    "glob": "^10.4.1",
    "gsap": "^3.12.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "svgo": "^3.2.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.21",
    "vue-clipboard3": "^2.0.0",
    "vue-m-message": "^4.0.2"
  },
  "exports": {
    ".": {
      "import": "./packages/es/index.js",
      "require": "./packages/cjs/index.js"
    },
    "./icons.json": {
      "import": "./icons.json",
      "require": "./icons.json"
    },
    "./styles/*.css": {
      "import": "./styles/*.css",
      "require": "./styles/*.css"
    }
  },
  "clean-publish": {
    "fields": [
      "config",
      "author"
    ],
    "packageManager": "yarn"
  },
  "config": {
    "style": "fill",
    "size": "24"
  }
}